  "description": "API Desenvolvida utilizando TS, Node, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "bld": "tsc -p .",
    "builded": "cross-env NODE_ENV=development node ./build/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts ./tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts ./tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
